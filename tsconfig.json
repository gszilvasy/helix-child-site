{
    "include": ["src/blocks/**/*", "src/shared/**/*"],
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "composite": true,
      "declaration": true,
      "declarationMap": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "module": "esnext",
      "moduleResolution": "nodenext",
      "skipLibCheck": true,
      "sourceMap": true,
      "strict": false,
      "strictNullChecks": false,
      "target": "es2017",
      "outDir": "dist"
    }
}